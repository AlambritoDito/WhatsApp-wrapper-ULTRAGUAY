{
  "name": "@brito/whatsapp-wrapper-ultra",
  "version": "1.0.1",
  "description": "Modular TypeScript wrapper for WhatsApp Cloud API (Meta).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "image-echo-bot": "ts-node-dev --respawn scripts/imageEchoBot.ts",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY.git"
  },
  "bugs": {
    "url": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY/issues"
  },
  "homepage": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY#readme",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "@aws-sdk/client-s3": "^3.479.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "nock": "^13.3.0",
    "aws-sdk-client-mock": "^3.0.0"
  }
}
