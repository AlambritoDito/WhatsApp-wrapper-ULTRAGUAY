{
  "name": "@whatsapp-wrapper-ultraguay/whatsapp-wrapper-ultraguay",
  "version": "2.0.0-alpha.1",
  "description": "Instanciable, type‑safe TypeScript wrapper for the WhatsApp Cloud API (Meta). Zero business logic — pure protocol wrapper.",
  "keywords": [
    "whatsapp",
    "cloud api",
    "meta",
    "bot",
    "typescript",
    "wrapper",
    "sdk",
    "whatsapp-business"
  ],
  "author": "Alan Pérez Fernández <alan@example.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./webhook": {
      "types": "./dist/webhook/index.d.ts",
      "require": "./dist/webhook/index.js",
      "import": "./dist/webhook/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "require": "./dist/storage/index.js",
      "import": "./dist/storage/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "require": "./dist/testing/index.js",
      "import": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --forceExit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY.git"
  },
  "bugs": {
    "url": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY/issues"
  },
  "homepage": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY#readme",
  "license": "MIT",
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.39.1"
  }
}
