{
  "name": "@brito/whatsapp-wrapper-ultra",
  "version": "2.0.0",
  "description": "General-purpose TypeScript wrapper for the WhatsApp Cloud API (Meta). Instanciable, zero singletons, native fetch.",
  "keywords": [
    "whatsapp",
    "cloud-api",
    "meta",
    "bot",
    "typescript",
    "sdk"
  ],
  "author": "Alan Pérez Fernández",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY.git"
  },
  "bugs": {
    "url": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY/issues"
  },
  "homepage": "https://github.com/AlambritoDito/WhatsApp-wrapper-ULTRAGUAY#readme",
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.400.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.479.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "aws-sdk-client-mock": "^3.0.0",
    "express": "^4.18.2",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
